{
  "name": "magicui",
  "version": "0.1.1",
  "private": true,
  "type": "module",
  "scripts": {
    "prefix:cli": "pnpm --prefix ./packages/cli/ -- ",
    "install:cli": "pnpm prefix:cli install",
    "dev:cli": "pnpm prefix:cli dev",
    "build:cli": "pnpm prefix:cli build",
    "release:cli": "pnpm prefix:cli release",
    "preinstall": "npx only-allow pnpm",
    "dev": "next dev",
    "prebuild": "pnpm build:docs && pnpm build:registry",
    "build": "next build",
    "build:registry": "npx tsx ./scripts/build-registry.mts",
    "registry:build": "tsx --tsconfig ./tsconfig.scripts.json ./scripts/build-registry.mts && prettier --log-level silent --write \"registry/**/*.{ts,tsx,json,mdx}\" --cache",
    "shadcn:build": "shadcn build",
    "build:docs": "content-collections build",
    "typecheck": "pnpm build:docs && tsc --noEmit",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\""
  },
